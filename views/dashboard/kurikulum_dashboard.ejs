<div class="pt-12 pl-9 flex">
    <div>
        <h1 class="text-display_sm font-medium">Kurikulum Tingkatan</h1>
        <p class="text-text_md font-regular text-gray-500">Atur kurikulum dan mata pelajaran setiap tingkatan</p>
    </div>
</div>

<div class="pl-8 pr-14 pt-5">
    <div class="overflow-x-auto relative shadow-md sm:rounded-lg">
        <table class="w-full text-sm text-left text-gray-500">
            <caption class="p-5 font-semibold text-text-base bg-white">
                <div class="flex items-center">
                    <h1 class="text-left flex-1">Tingkat 10</h1>
                    <div>
                        <button type="button" class="bg-white text-text-base text-text_sm font-semibold rounded-lg
                        border border-gray-300 px-4 py-3 mr-3">Upload CSV</button>
                        <a href="/kurikulum/tambah_jurusan">
                            <button type="button" class="bg-primary-600 text-white text-text_sm rounded-lg text-center inline-flex items-center px-4 py-3">Tambah Jurusan</button>
                        </a>
                    </div>
                </div>
            </caption>
            <thead class="text-text_sm font-medium text-gray-500 bg-gray-50">
                <tr>
                    <th scope="col" class="py-3 px-6">
                        Jurusan
                    </th>
                    <th scope="col" class="py-3 px-6">
                        Semester
                    </th>
                    <th scope="col" class="py-3 px-6">
                        Standar Kurikulum
                    </th>
                    <th scope="col" class="py-3 px-6">
                        Skala Penilaian
                    </th>
                    <th scope="col" class="py-3 px-6">
                        Date Uploaded
                    </th>
                    <th scope="col" class="py-3 px-6">
                        Last Updated
                    </th>
                    <th scope="col" class="py-3 px-6">
                        <span class="sr-only"></span>
                    </th>
                </tr>
            </thead>
            <tbody>
                <% dataKurikulum.forEach(kurikulum => { %>
                    <tr class="bg-white border-b">
                        <th scope="row" class="py-7 px-8 font-medium text-text-base whitespace-nowrap">
                            <%= kurikulum.jurusan %> 
                        </th>
                        <td class="py-7 px-8 ">
                            <%= kurikulum.semester %>
                        </td>
                        <td class="py-7 px-8 ">
                            <%= kurikulum.standarKurikulum %>
                        </td>
                        <td class="py-7 px-8 ">
                            <%= kurikulum.skalaPenilaian %>
                        </td>
                        <td class="py-7 px-8 ">
                            Jan 4, 2022
                        </td>
                        <td class="py-7 px-8 ">
                            Jan 4, 2022
                        </td>
                        <td class="py-7 px-8 text-right">
                            <button id="dropdownMenuIconButton" data-dropdown-toggle="dropdownDotsMenu" type="button" class="group">
                                <img src="/assets/more-vertical.png" alt="" style="height:20px ; width: 20px;">
                            
                            <!-- dropdown menu -->
                                <div id="dropdownDotsMenu" class="absolute hidden group-focus:block z-10 w-44 bg-white rounded divide-y shadow right-0">
                                    <ul class="py-1 text-sm text-text-base dark:text-gray-200" aria-labelledby="dropdownMenuIconButton">
                                        <li>
                                        <a href="/kurikulum/edit/<%= kurikulum.id_jurusan %>" class="block py-2 px-4 hover:bg-gray-100 text-text-base">Edit Informasi Jurusan</a>
                                        </li>
                                        <li>
                                        <a href="/kurikulum/<%= kurikulum.id_jurusan %>" class="block py-2 px-4 hover:bg-gray-100 text-text-base">Tambah Mata Pelajaran</a>
                                        </li>
                                        <li>
                                        <a href="/kurikulum/delete/<%= kurikulum.id_jurusan %>" class="block py-2 px-4 hover:bg-gray-100 text-text-base">Hapus Jurusan</a>
                                        </li>
                                    </ul>
                                </div>
                            </button>
                        </td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
</div>

